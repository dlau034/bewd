<h1>Edit Bicycle</h1>

<%= form_for @bicycle_order do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :wheel_set %>
    <%= f.select :wheel_set_id, @wheel_sets.map { |wheel_set| [wheel_set.name, wheel_set.id] }, include_blank: 'Default' %>
  </div>

  <%= f.submit %>
<% end %>